|       <= [0, 2, 4, 5] |    [0, 1, 3, 6]    |    [0, 4, 5, 6]    |
|          <= [0, 1, 6] |   [0, 1, 4, 5, 6]  |   [0, 2, 4, 5, 6]  |
|    <= [0, 2, 4, 5, 6] |    [0, 1, 3, 6]    |    [0, 4, 5, 6]    |
|       <= [0, 1, 3, 6] | [0, 1, 3, 4, 5, 6] | [0, 2, 3, 4, 5, 6] |
|       <= [0, 4, 5, 6] |      [0, 1, 6]     |    [0, 4, 5, 6]    |
